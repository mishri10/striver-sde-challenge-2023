class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        if(n==0)return;
        int j=m;
        for(int i=0;i<n;i++)
            nums1[j++]=nums2[i];
        sort(nums1.begin(),nums1.end());
        return;
    }
};
//T.C:O(N);S.C:O(1)
// SOLUTION ACCORDING TO LEETCODE PROBLEM
